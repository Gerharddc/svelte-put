{
  "name": "@svelte-put/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "code:build": "pnpm code:process && pnpm code:format",
    "code:process": "ts-node ./scripts/convert-code-files.ts",
    "code:format": "prettier --write \"src/**/*.code.js.svelte\"",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --check --ignore-path ../../.prettierignore \"./**/*.{ts,js,cjs,json,svelte,css}\""
  },
  "dependencies": {
    "@vercel/analytics": "^0.1.6",
    "clsx": "^1.2.1",
    "cookie": "^0.5.0",
    "handlebars": "^4.7.7",
    "static-tree": "^1.2.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.2.0",
    "@svelte-put/avatar": "workspace:*",
    "@svelte-put/clickoutside": "workspace:*",
    "@svelte-put/copy": "workspace:*",
    "@svelte-put/dragscroll": "workspace:*",
    "@svelte-put/intersect": "workspace:*",
    "@svelte-put/modal": "workspace:*",
    "@svelte-put/movable": "workspace:*",
    "@svelte-put/preprocess-auto-slug": "workspace:*",
    "@svelte-put/select": "workspace:*",
    "@svelte-put/shortcut": "workspace:*",
    "@svelte-put/toc": "workspace:*",
    "@sveltejs/adapter-vercel": "1.0.0-next.85",
    "@sveltejs/kit": "1.0.0-next.582",
    "@tailwindash/triangle": "^1.0.5",
    "@tailwindcss/typography": "^0.5.8",
    "@types/cookie": "^0.5.1",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vnphanquang/prettierrc": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.29.0",
    "glob": "^8.0.3",
    "highlight.js": "^11.7.0",
    "postcss": "^8.4.19",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "satori": "^0.0.44",
    "satori-html": "^0.3.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.2",
    "svelte-highlight": "^6.2.1",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  },
  "type": "module"
}
