{
  "name": "svelte-put",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/actions/*",
    "packages/configs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": "8.5.0",
    "node": "16.14.2"
  },
  "packageManager": "pnpm@7.0.0"
}
