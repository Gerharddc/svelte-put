{
  "name": "@svelte-put/movable",
  "version": "1.1.2",
  "description": "Action to move node on mousedown & mousemove",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "lint": "eslint --ignore-path .gitignore \"./**/*/*{ts,js,cjs,json}\"",
    "format": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"./**/*.{ts,js,cjs,json,css}\"",
    "build": "rollup -c",
    "semantic-release": "semantic-release",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown -i api/temp -o api/docs",
    "api": "run-s api:extract api:document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnphanquang/svelte-action-movable.git"
  },
  "keywords": [
    "svelte",
    "action",
    "drag",
    "move",
    "movable",
    "modal",
    "dialog",
    "draggable",
    "mousedown"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-action-movable/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-action-movable#readme",
  "devDependencies": {

  }
}
